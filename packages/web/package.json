    "react-dom": "^18.2.0",
    "swr": "2.0.0-beta.7"
  },
  "devDependencies": {
    "@hvmonsterdb/types": "workspace:*",
    "@parcel/reporter-bundle-analyzer": "^2.8.0",
    "@types/react": "^18.0.25",
